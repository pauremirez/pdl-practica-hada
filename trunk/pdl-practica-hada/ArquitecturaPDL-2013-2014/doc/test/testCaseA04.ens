; INICIO PROGRAMA PRINCIPAL HAda 
RES 11
MOVE #65535 , .SP
MOVE .SP , .IY
SUB  .SP , #8
MOVE .A , .SP
MOVE .IY , /0
WRSTR /cadena1
WRCHAR #10
WRCHAR #13
SUB #2, #1
MOVE .A, #-7[.IY]
; Traducir Quadruple - [MV x, T_0, null]
MOVE #-7[.IY], #-5[.IY]
WRSTR /cadena2
WRCHAR #10
WRCHAR #13
WRINT #-7[.IY]
WRCHAR #10
WRCHAR #13
; Traducir Quadruple - [MV t, 1, null]
MOVE #1, #-6[.IY]
WRSTR /cadena3
WRCHAR #10
WRCHAR #13
; Traducir Quadruple - [CMP t, 0, null]
CMP #-6[.IY], #0
; Salto si no cero a etiqueta L_0
BNZ /L_0
; Etiqueta de salto L_1
L_1 :
WRSTR /cadena4
WRCHAR #10
WRCHAR #13
BR /etiqueta1
; Salto si cero a etiqueta L_1
BZ /L_1
; Etiqueta de salto L_0
L_0 :
WRSTR /cadena5
WRCHAR #10
WRCHAR #13
; Etiqueta de salto etiqueta1
etiqueta1 :
HALT 

; Inicio Cadenas de Texto

cadena0: DATA "\n"
cadena1: DATA "VARIABLES Y ARITMETICA SIMPLE"
cadena2: DATA "x(1):"
cadena3: DATA "t(true):"
cadena4: DATA "false"
cadena5: DATA "true"
