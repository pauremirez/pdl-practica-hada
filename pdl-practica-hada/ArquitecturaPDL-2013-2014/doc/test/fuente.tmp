MOVE .A , .SP
MOVE #RET_L_4 , #-1[.IY]
MOVE .IX , #-3[.IY]
MOVE #REF_L_4 , #-4[.IY]
BR /L_0
REF_L_4:
BR /REF_L_0
RET_L_4: 
MOVE .R9 , #-5[.IY]

; Etiqueta de salto L_5
L_5 :
NOP
WRINT #-7[.IY]
WRCHAR #10
WRCHAR #13
L_1 : 
; Retorno Argumentos REFERENCIA 
MOVE #-4[.IY] , .R5 
MOVE .R5 , .PC 
REF_L_0: 
; Retorno Subprograma 
