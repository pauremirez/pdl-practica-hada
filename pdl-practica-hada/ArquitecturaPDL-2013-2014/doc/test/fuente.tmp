MOVE .A , .SP
MOVE .IY , /0
;-- Definicion FUNCION/PROCEDIMIENTO 
BR /FIN_L_0
L_0 :

; Traducir Quadruple - [MV y, x, null]
MOVE #-6[.IY], #-7[.IY]
